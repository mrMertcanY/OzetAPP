
<!DOCTYPE html>
<html>
<head>
    <!-- Basic Page Info -->
    <meta charset="utf-8">
    <title>Özet App</title>

    <!-- Site favicon -->
    <link rel="apple-touch-icon" sizes="180x180" href="~/deskapp-master/vendors/images/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/deskapp-master/vendors/images/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/deskapp-master/vendors/images/favicon-16x16.png">

    <!-- Mobile Specific Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
    <!-- CSS -->
    <link rel="stylesheet" type="text/css" href="~/deskapp-master/vendors/styles/core.css">
    <link rel="stylesheet" type="text/css" href="~/deskapp-master/vendors/styles/icon-font.min.css">
    <link rel="stylesheet" type="text/css" href="~/deskapp-master/src/plugins/datatables/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" type="text/css" href="~/deskapp-master/src/plugins/datatables/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" type="text/css" href="~/deskapp-master/vendors/styles/style.css">
    <link rel="stylesheet" type="text/css" href="~/deskapp-master/src/plugins/dropzone/src/dropzone.css">
    <!-- switchery css -->
    <link rel="stylesheet" type="text/css" href="~/deskapp-master/src/plugins/switchery/switchery.min.css">
    <!-- bootstrap-tagsinput css -->
    <link rel="stylesheet" type="text/css" href="~/deskapp-master/src/plugins/bootstrap-tagsinput/bootstrap-tagsinput.css">
    <!-- bootstrap-touchspin css -->

    <link rel="stylesheet" type="text/css" href="~/deskapp-master/src/plugins/bootstrap-touchspin/jquery.bootstrap-touchspin.css">
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>




    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-119386393-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-119386393-1');
    </script>
</head>
<body>
    <div class="header">
        <div class="header-left">
            <div class="menu-icon dw dw-menu"></div>


        </div>
        <div class="header-right">


            <div class="user-info-dropdown">
                <div class="dropdown">
                    <a class="dropdown-toggle" href="#" role="button" data-toggle="dropdown">
                        <span class="user-icon">
                            <img style="height:100%" src="@Session["userPhoto"]" alt="">
                        </span>
                        <span class="user-name">@Session["userName"]</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right dropdown-menu-icon-list">
                        <a class="dropdown-item" href="/Profile/Index"><i class="dw dw-user1"></i> Profil</a>
                        <a class="dropdown-item" href="/Settings/Index"><i class="dw dw-settings2"></i> Ayarlar</a>
                        <a class="dropdown-item" href="@Url.Action("Logout","Login")"><i class="dw dw-logout"></i> Çıkış Yap</a>
                    </div>
                </div>
            </div>

        </div>
    </div>


    <div class="left-side-bar">
        <div class="brand-logo">
            <a href="/Home/Index">
                <img src="~/deskapp-master/vendors/images/ozetapplogo.png" alt="" class="light-logo">
            </a>
            <div class="close-sidebar" data-toggle="left-sidebar-close">
                <i class="ion-close-round"></i>
            </div>
        </div>
        <div class="menu-block customscroll">
            <div class="sidebar-menu">
                <ul id="accordion-menu">
                    <li>
                        <a href="/Home/Index" class="dropdown-toggle no-arrow">
                            <span class="micon dw dw-house-1"></span><span class="mtext">Ana Sayfa</span>
                        </a>
                    </li>

                    <li>
                        <a href="/Transactions/Index" class="dropdown-toggle no-arrow">
                            <span class="micon dw dw-analytics-21"></span><span class="mtext">İşlemler</span>
                        </a>
                    </li>

                    <li>
                        <a href="/Category/Index" class="dropdown-toggle no-arrow">
                            <span class="micon dw dw-folder"></span><span class="mtext">Kategoriler</span>
                        </a>
                    </li>


                    <li>
                        <a href="/HouseholdTimeline/Index" class="dropdown-toggle no-arrow">
                            <span class="micon dw dw-wall-clock1"></span><span class="mtext">Hane Zaman Şeridi</span>
                        </a>
                    </li>


                    <li>
                        <a href="/Household/Index" class="dropdown-toggle no-arrow">
                            <span class="micon dw dw-diagram"></span><span class="mtext">Hane</span>
                        </a>
                    </li>

                    <li>
                        <a href="/Summary/Index" class="dropdown-toggle no-arrow">
                            <span class="micon dw dw-invoice"></span><span class="mtext">Özet</span>
                        </a>
                    </li>

                </ul>
            </div>
        </div>
    </div>

    <div class="main-container">
        @RenderBody()
    </div>
    <!-- js -->
    <script src="~/deskapp-master/vendors/scripts/core.js"></script>
    <script src="~/deskapp-master/vendors/scripts/script.min.js"></script>
    <script src="~/deskapp-master/vendors/scripts/process.js"></script>
    <script src="~/deskapp-master/vendors/scripts/layout-settings.js"></script>
    <script src="~/deskapp-master/src/plugins/apexcharts/apexcharts.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/dataTables.responsive.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/responsive.bootstrap4.min.js"></script>
    <script src="~/deskapp-master/vendors/scripts/dashboard.js"></script>
    <script src="~/deskapp-master/vendors/scripts/datatable-setting.js"></script>

    <script src="~/deskapp-master/src/plugins/datatables/js/dataTables.buttons.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/buttons.bootstrap4.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/buttons.print.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/buttons.html5.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/buttons.flash.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/pdfmake.min.js"></script>
    <script src="~/deskapp-master/src/plugins/datatables/js/vfs_fonts.js"></script>

    <script src="~/deskapp-master/vendors/scripts/advanced-components.js"></script>
    <script src="~/deskapp-master/src/plugins/bootstrap-touchspin/jquery.bootstrap-touchspin.js"></script>

    <script src="~/deskapp-master/src/plugins/sweetalert2/sweetalert2.all.js"></script>
    <script src="~/deskapp-master/src/plugins/sweetalert2/sweet-alert.init.js"></script>

    <script src="~/deskapp-master/src/plugins/dropzone/src/dropzone.js"></script>
</body>
</html>
